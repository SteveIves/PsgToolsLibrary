
namespace SynPSG.VmsCompatibility

	subroutine lib$get_symbol
		required in symbol      ,a
		required out value      ,a
		endparams
		stack record
			len             ,i4
		endrecord
	proc
		xcall getlog(symbol,value,len)
		if (!len)
			clear value
		xreturn
	endsubroutine

endnamespace

